    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="200" />
      <inertia ixx="66.66666666666666" ixy="0" ixz="0" iyy="100" iyz="0" izz="66.66666666666666" />
    </inertial>
  </link>
  <joint name="base_link_to_imu" type="fixed">
    <parent link="base_link" />
    <child link="imu" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>
  <link name="imu">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
