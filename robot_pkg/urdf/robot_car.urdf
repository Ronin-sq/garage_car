                        <max>3.5</max>
                        <resolution>0.015</resolution>
                    </range>
                </scan>
                <noise>
                    <type>gaussian</type>
                    <mean>0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>
        </sensor>
